<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
		<record id="view_product_category_consumable_form" model="ir.ui.view">
            <field name="name">product.category.consumable.form.inherit</field>
            <field name="model">product.category</field>
            <field name="inherit_id" ref="product.product_category_form_view"/>
            <field name="arch" type="xml">
                <group name="first" position="after">
                    <group>
                        <group name="stock_consumable" string="Stock">
                            <field name="is_stockable_consumable"/>
                        </group>
                    </group>
                </group>
            </field>
        </record>
    </data>
</openerp>